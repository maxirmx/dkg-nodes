server {
  include       mime.types;
  default_type  application/octet-stream;

  listen 80 ;
  listen [::]:80 ;
  # listen 443 ssl ;
  # listen [::]:443 ssl ;
  # ssl_certificate /etc/nginx/certificate/s.crt;
  # ssl_certificate_key /etc/nginx/certificate/s.key;
  root /var/www/dkg;
  index index.html;

  server_name _ ;

  location / {
    try_files $uri /index.html =404;
  }
}
